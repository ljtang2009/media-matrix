{
  "name": "media-matrix",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build:dev": "ts-node build/build.dev.ts",
    "build:prod": "npm run type-check && ts-node build/build.prod.ts",
    "preview": "ts-node build/preview.ts",
    "type-check": "vue-tsc --noEmit -p src/tsconfig.json --composite false",
    "lint": "npm run eslint && npm run stylelint:no-color",
    "eslint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "eslint:fix": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "stylelint": "stylelint **/*.*ss",
    "stylelint:no-color": "stylelint **/*.*ss --no-color",
    "stylelint:fix": "stylelint **/*.*ss --fix",
    "test": "node test/index.js",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ljtang2009/media-matrix.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
